# ------------------------
# Twitter
# ------------------------
# JSON string of cookies for Twitter API access
TWITTER_COOKIES={"auth_token":"XXXX","ct0":"XXXX"}
TWITTER_LIST_QUERY=query1 query2 # Space-separated list of queries/keywords to scrape
TWITTER_SCRAPE_SLEEP_TIME=10 # Delay between scrape cycles (seconds)
TWITTER_SLA=60 # Service level agreement time window (seconds)
TWITTER_TWEETS_COUNT=20 # Number of tweets to fetch per query
TWITTER_ENABLED=true # Enable Twitter crawler? true/false

# ------------------------
# Threads
# ------------------------
THREADS_LIST_USERNAME=username1 username2 # Space-separated list of Threads usernames to scrape
THREADS_SCRAPE_SLEEP_TIME=10 # Delay between scrape cycles (seconds)
THREADS_SLA=60 # SLA for processing Threads
THREADS_ENABLED=true # Enable Threads crawler? true/false

# ------------------------
# Telegram
# ------------------------
TELEGRAM_API_ID=123456          # Telegram API ID from my.telegram.org
TELEGRAM_API_HASH=API_HASH_XXXX # Telegram API hash
TELEGRAM_SESSION_STRING=SESSION_STRING_XXXX # Telegram session string
TELEGRAM_BOT_TOKEN=BOT_TOKEN_XXXX # Telegram bot token
TELEGRAM_BOT_TRADING_TOKEN=BOT_TRADING_TOKEN_XXXX
TELEGRAM_ENABLED=true          # Enable Telegram integration
TELEGRAM_LIMIT=100              # Max messages to fetch per scrape
TELEGRAM_LIST_CHANNEL=channel1 channel2 # Channels to monitor
TELEGRAM_NEWS_PEER_ID=111111111 # Chat ID for news notifications
TELEGRAM_TRADE_PEER_ID=222222222 # Chat ID for trade notifications
TELEGRAM_LOG_PEER_ID=333333333   # Chat ID for error logs
TELEGRAM_ALERT_CHAT_ID=666666666 # Chat ID for alerts
TELEGRAM_GROUP_CHAT_ID=555555555 # Chat ID for trading group
TELEGRAM_PNL_CHAT_ID=444444444   # Chat ID for PnL updates
TELEGRAM_ROI_SIGNAL=5.5          # Minimum ROI for trade signals
TELEGRAM_SCRAPE_SLEEP_TIME=10
TELEGRAM_SLA=60
TELEGRAM_ME=me_username

# ------------------------
# Discord
# ------------------------
DISCORD_ENABLED=true
DISCORD_SLA=60
DISCORD_LIMIT=50
DISCORD_SCRAPE_SLEEP_TIME=10
DISCORD_LIST_CHANNEL_ID=channel_id_1 channel_id_2
DISCORD_TOKEN=DISCORD_TOKEN_XXXX

# ------------------------
# Binance
# ------------------------
BINANCE_API_KEY=BINANCE_API_KEY_XXXX
BINANCE_API_SECRET=BINANCE_API_SECRET_XXXX
BINANCE_TLD=com
BINANCE_PROXY_URL=http://host:port  # Fill empty string if no proxy

# ------------------------
# Tor Proxy
# ------------------------
TOR_PROXY_URL=socks5://user:pass@host:port # Fill empty string if not use
TOR_PROXY_NUM_PORTS=1

# ------------------------
# Telegram Proxy
# ------------------------
TELEGRAM_PROXY_URL=http://host:port      # HTTP or socks5 proxy
TELEGRAM_PROXY_NUM_PORTS=1

# ------------------------
# General
# ------------------------
TIMEZONE=Asia/Ho_Chi_Minh
